{
  "name": "@mck-p/groworks-cms",
  "version": "0.0.0-DEVELOPMENT",
  "description": "The Content Management System for GroWorks",
  "main": "start.js",
  "author": "Tim Roberts <timroberts@fastmail.org>",
  "license": "MIT",
  "scripts": {
    "build::client": "webpack --config ./config/webpack.client.js",
    "build::server": "webpack --config ./config/webpack.server.js",
    "build": "yarn clear && yarn build::client && yarn build::server",
    "dev::client": "yarn build::client --watch",
    "dev::server": "concurrently --kill-others -n \"WEBPACK,NODEMON\" \"yarn build::server --watch\" \"nodemon --watch build/ -r dotenv-flow/config build/server/start.js\"",
    "dev": "yarn clear && yarn build && concurrently --kill-others -n \"WEBPACK,SERVER\" \"yarn dev::client\" \"yarn dev::server\"",
    "fmt": "prettier --write \"{server,client}/**/*.js\"",
    "clear": "rm -rf build dist",
    "start": "node build/server/start.js"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "getenv": "^1.0.0",
    "helmet": "^3.23.3",
    "jsonschema": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "pg": "^8.3.0",
    "pino": "^6.3.2",
    "pino-pretty": "^4.0.1",
    "ramda": "^0.27.0",
    "rate-limit-redis": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.4",
    "autoprefixer": "^9.8.5",
