    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.0"
  },
  "browsrslist": [
